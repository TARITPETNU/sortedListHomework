<Window x:Class="sortedListhomework.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:sortedListhomework"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="keyTxt" HorizontalAlignment="Left" Height="21" Margin="112,165,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244" RenderTransformOrigin="0.5,0.5">
            <TextBox.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform AngleX="-3.846"/>
                    <RotateTransform/>
                    <TranslateTransform X="-0.706"/>
                </TransformGroup>
            </TextBox.RenderTransform>
        </TextBox>
        <TextBox x:Name="valueTxt" HorizontalAlignment="Left" Height="23" Margin="112,191,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="244"/>
        <Button x:Name="Btn" Content="add" HorizontalAlignment="Left" Margin="361,165,0,0" VerticalAlignment="Top" Width="75" Height="49" Click="Btn_Click"/>

    </Grid>
</Window>
